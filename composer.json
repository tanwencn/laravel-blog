{
    "name": "tanwencn/laravel-cms",
    "description": "TanwenCms",
    "type": "library",
    "keywords": ["laravel", "cms", "blog", "news", "ecommerce", "shop", "mall"],
    "homepage": "https://github.com/tanwencn/laravel-cms",
    "license": "MIT",
    "authors": [
        {
            "name": "tanwen",
            "email": "361657055@qq.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.5.0|~5.6.0",
        "genealabs/laravel-model-caching": "^0.2.52",
        "studio-42/elfinder": "~2.1.10",
        "barryvdh/elfinder-flysystem-driver": "^0.1.4|^0.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "symfony/dom-crawler": "^2.7|^3.0|^4.0",
        "arrilot/laravel-widgets": "^3.12",
        "igaster/laravel-theme": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0",
        "laravel/framework": "~5.5.0|~5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Tanwencn\\Cms\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanwencn\\Cms\\CmsServiceProvider"
            ],
            'aliases': {
                'Admin': "Tanwencn\\Cms\\Facades\\AdminFacade"
            }
        }
    }
}
